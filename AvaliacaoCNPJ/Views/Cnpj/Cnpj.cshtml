@model AvaliacaoCNPJ.Models.ViewModels.CnpjFormViewModel
@{
    ViewData["Title"] = "Cnpj";
}
<h2>Cnpj</h2>
<form action="~/Cnpj/Save" method="post">
    <div class="row">



