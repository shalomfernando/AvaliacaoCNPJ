                        <th>
                            Texto
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.Empresa.Atividade_principal)
                    {
                        <tr>
                            <td>
                                @Html.TextBoxFor(modelItem => item.Code)
